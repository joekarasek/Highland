<!DOCTYPE html>
<html dir="ltr" lang="en-US">

{% include '/organisms/head.twig' %}

<body class="stretched">

<!-- Document Wrapper
============================================= -->
<div id="wrapper" class="clearfix">

    {% include 'organisms/header.twig' with
    {
    "transparent": false
    }
    %}

    {% include 'organisms/marquee.twig' %}

    <!-- Content
  ============================================= -->

    <div id="content" >

        {% include 'organisms/video-slab.twig' %}

        {% include 'molecules/cta__two-to-one.twig' with {
            id: 'cta-one',
            existingPatients: true
        } %}

        {% include 'organisms/about.twig' %}

        {% include 'organisms/new_specials.twig' %}

        {% include 'molecules/cta__two-to-one.twig' with {
            id: 'cta-two',
            existingPatients: false,
            shadow: false,
            classes: ""
        } %}

        <section id="section-award" class="award">
            <img src="/assets/award-bg-mobile.jpg" alt="Award Winning Dental Care" class="hidden-lg hidden-md hidden-sm">
            <div class="container">
                <h2 class="award__title">AWARD WINNING</h2>
                <h2 class="award__title--sub">dental care</h2>
                <img src="/assets/award-logos.png" alt="Seattle Magazine voted best dentist 2009-2016, Seattle Met 2016 Top Dentist">
            </div>
        </section>

        {% include 'organisms/reviews.twig' %}

        {% include 'organisms/insurance.twig' %}

        {% include 'organisms/services.twig' %}

        {% include 'molecules/cta__two-to-one.twig' with {
        id: 'cta-three',
        existingPatients: true,
        shadow: false,
        classes: "flipped"
        } %}

        {% include 'organisms/forms.twig' %}

        {% include 'organisms/smile-gallery.twig' %}

        {%  include 'organisms/tour.twig' %}

        {% include 'organisms/location--floating-card.twig' %}

    </div>

    <!-- Modals
    ============================================= -->
    {% include 'organisms/request-modal.twig' %}

    <!-- Footer
    ============================================= -->
    {% include 'organisms/footer.twig' %}

</div><!-- #wrapper end -->

{% include 'organisms/foot.twig' %}

</body>
</html>